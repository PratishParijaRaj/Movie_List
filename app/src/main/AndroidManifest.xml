<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pratishparija.recycle">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".Movie_Details" />
        <activity android:name=".ScrollViewImp"
            android:theme="@style/AppTheme" />
        <activity android:name=".MyTable" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".Report" />
        <activity android:name=".TabsActivity"
            android:theme="@android:style/Theme.Holo" />
        <meta-data  android:name="com.facebook.sdk.ApplicattionId"
            android:value="@string/fb_app_id"/>
    </application>

</manifest>